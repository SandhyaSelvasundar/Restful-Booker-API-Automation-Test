							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"const response = pm.response.json();\r",
									"\r",
									"pm.test(\"Body matches string - firstname as Person\", function () {\r",
									"    pm.expect(response.firstname).to.equal(\"Person\");\r",
									"});\r",
									"\r",
									"pm.test(\"Content-Type is present\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\", \"application/json; charset=utf-8\");\r",
									"});\r",
									"\r",
									"pm.test(\"Response time is less than 200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"pm.test(\"JSON Value Check - lastname as Test\", function () {\r",
									"    pm.expect(response.lastname).to.eql(\"Test\");\r",
									"});\r",
									"\r",
									"pm.test(\"JSON Value Check - checkout\", function () {\r",
									"    pm.expect(response.bookingdates.checkout).to.eql(\"2024-06-25\");\r",
									"});\r",
									""
